{
  "index_patterns": ["credentials-*"] ,
  "template": {
    "settings": {
      "index": {
        "number_of_shards": 1,
        "number_of_replicas": 0,
        "refresh_interval": "5s"
      }
    },
    "mappings": {
      "properties": {
        "@timestamp": { "type": "date" },
        "message": { "type": "text" },
        "credential": {
          "properties": {
            "category": { "type": "keyword" },
            "data": { "type": "text" },
            "origin": { "type": "keyword" },
            "source": { "type": "keyword" },
            "type": { "type": "keyword" },
            "username": { "type": "keyword" }
          }
        },
        "c2": {
          "properties": {
            "program": { "type": "keyword" },
            "operator": { "type": "keyword" },
            "teamserver": { "type": "keyword" }
          }
        },
        "beacon.id": { "type": "keyword" },
        "tags": { "type": "keyword" }
      }
    }
  }
}
